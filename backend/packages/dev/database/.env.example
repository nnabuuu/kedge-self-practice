# Database Configuration
# REQUIRED: PostgreSQL connection URL for Hasura
HASURA_GRAPHQL_DATABASE_URL=postgresql://postgres:postgres@postgres:5432/kedge_db

# Security Configuration
# REQUIRED: Admin secret for Hasura GraphQL Engine
# Generate a strong secret: openssl rand -hex 32
HASURA_GRAPHQL_ADMIN_SECRET=your-secure-admin-secret-here

# Optional Configuration
HASURA_VERSION=2.35.1
HASURA_GRAPHQL_SERVER_PORT=28717
KEDGE_POSTGRES_DATA=pgdata
DEV_NETWORK_NAME=kedge_network