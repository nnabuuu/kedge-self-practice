{
  "name": "practice",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/libs/practice/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/libs/practice",
        "main": "packages/libs/practice/src/index.ts",
        "tsConfig": "packages/libs/practice/tsconfig.lib.json",
        "assets": ["packages/libs/practice/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/libs/practice/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/packages/libs/practice"],
      "options": {
        "jestConfig": "packages/libs/practice/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}